{
  "manifest_version": 2,
  "name": "Click n' Tweet",
  "description": "Tweet in one click within Chrome",
  "version": "1.4.5",
  "permissions": ["tabs"],
  "browser_action": {
    "default_icon": "icons/icon48.png",
    "default_title": "Tweet!"
  },
  "background": {
    "scripts": ["clickntweet.js"]
  },
  "icons":{
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
