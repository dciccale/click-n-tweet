{
  "name": "Click n' Tweet",
  "version": "1.4.3",
  "description": "Tweet in one click within Chrome",
  "manifest_version": 2,
  "permissions": ["tabs"],
  "browser_action": {
    "default_icon": "src/img/icon48.png",
    "default_title": "Tweet!"
  },
  "background": {
    "scripts": ["src/clickntweet.js"]
  },
  "icons":{
    "16": "src/img/icon16.png",
    "48": "src/img/icon48.png",
    "128": "src/img/icon128.png"
  }
}
