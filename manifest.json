{
  "name": "Click n' Tweet",
  "version": "1.3",
  "description": "Tweet current site or anything you want in 1 click",
  "manifest_version": 2,
  "permissions": [ "tabs" ],
  "browser_action": {
    "default_icon": "src/images/icon48.png",
    "default_popup": "src/clickntweet.html",
    "default_title": "Tweet!"
  },
  "icons":{
    "16": "src/images/icon16.png",
    "48": "src/images/icon48.png",
    "128": "src/images/icon128.png"
  }
}
